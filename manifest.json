{
  "name": "Jira ticket priority icon fix",
  "description": "Chrome extension to fix Jira ticket priority icons",
  "author": "Alexey Korepanov",
  "homepage_url": "https://github.com/alexkorep/jira-priority-extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://feelrobotics.atlassian.net/*"
      ],
      "js": ["injected.js"]
    }
  ],
  "icons": {
    "64": "icon.png"
  },
  "web_accessible_resources": [
    "medium.png"
  ],
  "manifest_version": 2
}
